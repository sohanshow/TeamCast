# xAI API Configuration (for Grok Imagine video generation)
# Get your API key from: https://console.x.ai
XAI_API_KEY=your_xai_api_key

# Google AI / Vertex AI Configuration (fallback for video, used for scene generation)
# Get your API key from: https://aistudio.google.com/apikey
GOOGLE_API_KEY=your_google_api_key

# Set to "true" to use Vertex AI instead of AI Studio
# GOOGLE_GENAI_USE_VERTEXAI=true

# LiveKit Configuration
# Get these from: https://cloud.livekit.io
LIVEKIT_URL=wss://your-livekit-instance.livekit.cloud
LIVEKIT_API_KEY=your_livekit_api_key
LIVEKIT_API_SECRET=your_livekit_api_secret

# App Configuration (optional)
NEXT_PUBLIC_APP_URL=http://localhost:3000
